    <!-- NUESTRO PROCESO DE GESTIÓN -->
    <section>
        <div class="container">
            <div class="section-title" style="text-align: center; max-width: 700px; margin: 0 auto 3rem;">
                <p class="subtitle">Metodología</p>
                <h2>Proceso de Gestión de Proyectos</h2>
